{% paginate blog.articles by 30 %}

<div class="blog-header">
    <div class="wrapper">
        <div class="desktop-b-menu grid">
            <div class="grid__item large--two-thirds">
                <a href="/blogs/learn" class="{% if current_tags %}{% else %}active{% endif %}">HOME</a>
                <a href="/blogs/learn/tagged/wellbeing" class="{% if current_tags.first == 'Wellbeing' %}active{% endif %}">WELLBEING</a>
              	<a href="/blogs/learn/tagged/pleasure" class="{% if current_tags.first == 'Pleasure' %}active{% endif %}">PLEASURE</a>
                <a href="/blogs/learn/tagged/relationships" class="{% if current_tags.first == 'Relationships' %}active{% endif %}">RELATIONSHIPS</a>
                <a href="/blogs/learn/tagged/technology" class="{% if current_tags.first == 'Technology' %}active{% endif %}">TECH</a>
            </div>          	
            <div class="grid__item large--one-third blog-search">
                <form action="/search" method="get" class="clearfix" role="search">
                    <input type="hidden" name="type" value="article">
                    <div class="input-group">
                        <span class="input-wrap">
                            <input type="search" name="q" value="" class="input-group-field"
                                   aria-label="Search" style="text-align: center;">
                        </span>
                        <span class="input-group-btn">
                            <button type="submit" class="btn icon-fallback-text">
                                <span class="icon icon-search" aria-hidden="true"></span>
                                <span class="fallback-text">Search</span>
                            </button>
                        </span>
                    </div>
                </form>
            </div>
        </div>      
    </div>
</div>


<div class="blog-page-wrap feature-wrap">
    <div class="wrapper">
        <div class="grid-uniform">

            <div class="grid__item blog-tag-title mobile-blog-menu"> 
                <div class="h1-wrap">
                    <h1 class="desktop-b-h1">
                    {% if current_tags %}
                      {{ current_tags.first }}
                    {% else %}
                      Home
                    {% endif %}
                   </h1>
                    <h1 class="mobile-b-h1">
                      {% if current_tags %}
                      <span> {{ current_tags.first }} ⌵</span>
                      {% else %}
                      <span>Home ⌵</span>
                      {% endif %}
                    </h1>
                  <div class="mobile-b-menu grid">
                      <div class="grid__item large--two-thirds">
                        <ul class="" style="display: none;">
                          <li><a href="/blogs/learn" class="active">HOME</a></li>
                          <li><a href="/blogs/learn/tagged/wellbeing">WELLBEING</a></li>
                          <li><a href="/blogs/learn/tagged/pleasure">PLEASURE</a></li>
                          <li><a href="/blogs/learn/tagged/relationships">RELATIONSHIPS</a></li>
                          <li><a href="/blogs/learn/tagged/technology">TECH</a></li>
                        </ul>  
                      </div>                      
                  </div>
                </div>
                <hr style="margin-bottom: 7px;">
            </div>
          	
          	{% assign four_count = 0 %}
			{% for article in blog.articles %}
          	{% if article.tags contains 'Home' and four_count < 4 %}
          	{% assign four_count = four_count | plus: 1 %}
          	{% assign read_time = '5 min read' %}
            {% for tag in article.tags %}
              {% assign read_time_pre = tag | truncate: 9, "" %}
              {% if read_time_pre == 'read-time' %}        		
              	{% assign read_time = '5 min read' | replace: '5', tag | replace: 'read-time-', '' %}
              {% endif %}
            {% endfor %}
            <div class="blog-special grid__item blog-article-item">

                <div class="rte">
                    <div class="blog-image">
                        {% if article.image %}
                        {% capture img_id %}ArticleImage-{{ section.id }}--{{ article.image.id }}{% endcapture %}
                        {% capture wrapper_id %}ArticleImageWrapper-{{ section.id }}--{{ article.image.id }}{% endcapture %}
                        {%- assign img_url = article.image | img_url: '1x1' | replace: '_1x1.', '_{width}x.' -%}
                        {%- assign image_alt = article.title | escape -%}
                        <div id="{{ wrapper_id }}" class="article__image-wrapper supports-js">
                          <div class="image-div" style="padding-top:{{ 1 | divided_by: article.image.aspect_ratio | times: 100}}%;">
                            <a href="{{ article.url }}">
                            <img id="{{ img_id }}"
                                 class="article__image lazyload"
                                 src="{{ article.image | img_url: '300x300' }}"
                                 data-src="{{ img_url }}"
                                 data-widths="[90, 120, 150, 180, 360, 480, 600, 750, 940, 1080, 1296]"
                                 data-aspectratio="{{ article.image.aspect_ratio }}"
                                 data-sizes="auto"
                                 alt="{{ image_alt }}">
                            </a>
                          </div>
                        </div>
                        <noscript>
                            <p>{{ article | img_url: '1024x1024' | img_tag: image_alt, 'article__image' | link_to: article.url }}</p>
                        </noscript>
                        {% endif %}
                    </div>
                    <h2><a href="{{ article.url }}">{{ article.title }}</a></h2>
                    <a href="{{ article.url }}">
                  	<div class="blog-article-summary">                    
                    {% if article.excerpt.size > 0 %}
                      {{ article.excerpt | strip_html | truncatewords: 20}}
                    {% else %}
                      {{ article.content | strip_html | truncatewords: 20 }}
                    {% endif %}                    
                    </div>
                  	</a>
                </div>
                <p class="blog-article-readmore">
                  {{ 'small-logo.svg' | asset_url | img_tag: 'Logo'}}
                  <a href="{{ article.url }}">{{ read_time }}</a>
                </p>
              
            </div>
         	{% endif %}
            {% endfor %}
          
          
          
        {% if paginate.pages > 1 %}
          <div class="text-center">
            {% include 'pagination-custom' %}
          </div>
        {% endif %}
          
        <div class="blog_border"><a href="#"></a></div> 
          
      </div>

        <div class="grid-uniform">
            <div class="grid__item" style="margin-top: -15px;">
                <div class="grid-uniform blog-get-guide">
                    <div class="grid__item large--three-fifths">
                        <h1>Stay in the know</h1>
                        <h2>
                            Sign up to thought-provoking articles, tips &amp; tricks,
                            exclusive offers and get our free MysteryPlaybook.
                        </h2>
                    </div>
                    <div class="grid__item large--two-fifths">
                        <div class="blog-get-guide-form">
                            <form id="blog-get-guide-form-1" action="//manage.kmail-lists.com/subscriptions/subscribe" data-ajax-submit="//manage.kmail-lists.com/ajax/subscriptions/subscribe" method="GET" target="_blank" class="clearfix">
                                <input type="hidden" name="g" value="PMkAdN">
                                <div class="input-group">
                                    <span class="input-wrap">
                                        <input type="email" name="email" value="" class="input-group-field" placeholder="Enter your email" autocorrect="off" autocapitalize="off" required>
                                    </span>
                                    <span class="input-group-btn">
                                        <button type="submit" class="btn icon-fallback-text klaviyo_submit_button">
                                            <span>Sign up</span>
                                        </button>
                                    </span>
                                </div>
                                <div class="klaviyo_messages">
                                    <div class="success_message" style="display:none;">
                                        <p class="" >Thanks for subscribing. Pleasure is on its way to your inbox.</p>
					                          </div>
                                    <div class="error_message" style="display:none;"></div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

