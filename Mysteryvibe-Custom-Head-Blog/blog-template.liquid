    if(search_value == ''){
      e.preventDefault();
    }
  });
  
  $("input[name='q']").keypress(function(e){
    var search_value = $(this).val();
    if(keycode == '13'){
      if(search_value == ''){
      	e.preventDefault();
      }
    }
  });
  
</script>  
